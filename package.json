{
  "name": "stackcite",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "deep-freeze": "0.0.1",
    "nock": "^9.0.2",
    "node-sass": "^4.5.0",
    "react-scripts": "0.8.1",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "normalizr": "^2.2.1",
    "react": "^15.4.1",
    "react-cookie": "^1.0.4",
    "react-copy-to-clipboard": "^4.2.3",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "scripts": {
    "build-css": "node-sass src/sass/ -o src/css",
    "watch-css": "node-sass src/sass/ -o src/css --watch",
    "start-js": "NODE_PATH=src/ react-scripts start",
    "build-js": "NODE_PATH=src/ react-scripts build",
    "test-js": "NODE_PATH=src/ react-scripts test --env=jsdom",
    "start": "npm run watch-css &! npm run start-js",
    "build": "npm run build-css && NODE_PATH=src/ react-scripts build",
    "test": "npm run test-js",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
